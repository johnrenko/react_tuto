{
  "name": "@myob/my-module-change-name",
  "version": "0.0.0",
  "description": "MYOB React Starter Template",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4.1.0"
  },
  "scripts": {
    "start": "npm run dev",
    "build": "webpack --config scripts/webpack.config.js --progress --colors",
    "dev": "webpack-dev-server --config scripts/webpack.config.js --progress --colors --hot --inline --history-api-fallback --content-base tmp/",
    "distrib": "webpack --config scripts/webpack.config.prod.js --display-modules --progress --colors",
    "stats": "webpack --config scripts/webpack.config.js --profile --json > stats.json",
    "test": "karma start scripts/karma.conf.js",
    "test-with-single-run": "karma start scripts/karma.conf.js --single-run",
    "test-in-browser": "karma start scripts/karma.conf.js no-single-run --browsers Chrome",
    "coverage": "karma start scripts/karma.cov.conf.js --single-run",
    "lib": "babel -d lib src/",
    "release": "npm run test-with-single-run && npm run lib && webpack --config scripts/webpack.config.prod.js --progress --colors && git commit -a -m \"Release: update distribution files\" && npm version $RELEASE -m \"Release of %s\" && npm publish && git push --tags origin master",
    "patch-release": "RELEASE=patch npm run release",
    "minor-release": "RELEASE=minor npm run release",
    "major-release": "RELEASE=major npm run release",
    "beta-release": "RELEASE=prerelease npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MYOB-Technology/react-starter-template.git"
  },
  "bugs": {
    "url": "https://github.com/MYOB-Technology/react-starter-template/issues"
  },
  "author": "MYOB - EX",
  "license": "UNLICENSED",
  "homepage": "https://github.com/MYOB-Technology/react-starter-template#readme",
  "publishConfig": {
    "registry": "http://npm.addevcloudservices.com.au"
  },
  "dependencies": {
    "@myob/myob-styles": "^0.3.11",
    "@myob/myob-widgets": "^0.2.1",
    "alt": "0.17.9",
    "babel-core": "5.8.29",
    "classnames": "2.2.0",
    "lodash.debounce": "3.1.1",
    "moment": "2.10.6",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "react-bootstrap": "0.28.1",
    "validate.js": "0.9.0"
  },
  "devDependencies": {
    "autoprefixer": "6.1.1",
    "babel": "5.8.29",
    "babel-eslint": "4.1.5",
    "babel-loader": "5.4.0",
    "compression-webpack-plugin": "0.2.0",
    "css-loader": "0.23.0",
    "eslint": "1.10.1",
    "eslint-loader": "1.1.1",
    "eslint-plugin-react": "3.10.0",
    "extract-text-webpack-plugin": "0.9.1",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "1.7.0",
    "imports-loader": "0.6.5",
    "isparta-loader": "1.0.0",
    "jasmine-core": "2.3.4",
    "karma": "0.13.15",
    "karma-chrome-launcher": "0.2.1",
    "karma-cli": "0.1.1",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.6",
    "karma-jasmine-ajax": "0.1.13",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-phantomjs-shim": "1.1.2",
    "karma-sourcemap-loader": "0.3.6",
    "karma-threshold-reporter": "0.1.15",
    "karma-webpack": "1.7.0",
    "node-libs-browser": "0.5.3",
    "node-sass": "3.4.2",
    "null-loader": "0.1.1",
    "phantomjs": "1.9.18",
    "postcss-loader": "0.8.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "transfer-webpack-plugin": "0.1.4",
    "url-loader": "0.5.6",
    "webpack": "1.12.8",
    "webpack-dev-server": "1.12.1"
  }
}
